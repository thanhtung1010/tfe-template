{
    "name": "tfe-template",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "serve:ssr:tfe-template": "node dist/tfe-template/server/server.mjs"
    },
    "prettier": {
        "printWidth": 100,
