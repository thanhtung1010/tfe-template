        "@angular/build": "21.0.4",
        "@angular/cli": "21.0.4",
        "@angular/compiler-cli": "21.0.0",
        "@tailwindcss/postcss": "4.1.18",
        "@types/express": "5.0.1",
        "@types/google-libphonenumber": "7.4.30",
        "@types/node": "20.17.19",
        "jsdom": "27.1.0",
        "postcss": "8.5.6",
        "tailwindcss": "4.1.18",
        "typescript": "5.9.2",
        "vitest": "4.0.8"
    }
